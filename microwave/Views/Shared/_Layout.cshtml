<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Teste Benner Microondas</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="row">
                <h1 class="title">Bem vindo ao Micro-ondas Benner</h1>
            </div>
            <h3 class="dev">Desenvolvedor: <b>Willian Rogerio Regina</b></h3>
        </div>
    </div>
</div>
    <div class="container body-content">
        @RenderBody()
    
        <footer>
            <p>&copy; @DateTime.Now.2020 - Microondas teste benner</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
